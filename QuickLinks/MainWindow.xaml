<Window x:Class="QuickLinks.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuickLinks"
        mc:Ignorable="d"
        Title="Quick Links - M.Franceschi" Height="200" Width="250">
    <Grid>
        <Button x:Name="GoogleLinkButton" Content="Google" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="223" Click="GoogleLinkButton_Click"/>
        <Button x:Name="MfranceschiGitHubLinkButton" Content="GitHub de Martin Franceschi" HorizontalAlignment="Left" Margin="10,65,0,0" VerticalAlignment="Top" Width="223" Click="MfranceschiGitHubLinkButton_Click"/>
        <Button x:Name="MfranceschiLinkedInLinkButton" Content="LinkedIn de Martin Franceschi" HorizontalAlignment="Left" Margin="10,89,0,0" VerticalAlignment="Top" Width="223" Click="MfranceschiLinkedInLinkButton_Click"/>
        <Label Content="Accéder à :" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,129,0,0" TextWrapping="WrapWithOverflow"  VerticalAlignment="Top" Width="223" TextAlignment="Center">
            (c) 2021 - Martin Franceschi<LineBreak />
            Tous droits réservés.
        </TextBlock>

    </Grid>
</Window>
